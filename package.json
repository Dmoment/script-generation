{
  "name": "app",
  "private": true,
  "scripts": {
    "build": "node esbuild.config.js",
    "build:css:compile": "sass ./app/assets/stylesheets/application.bootstrap.scss:./app/assets/builds/application.css --no-source-map --load-path=node_modules --quiet-deps",
    "build:css:prefix": "postcss ./app/assets/builds/application.css --output=./app/assets/builds/application.css",
    "build:css": "bun run build:css:compile && bun run build:css:prefix",
    "watch:css": "nodemon --watch ./app/assets/stylesheets/ --ext scss --exec \"bun run build:css\"",
    "generate:api-types": "rails api:generate_spec && openapi-ts --input ./public/api-spec-generated.json --output ./app/javascript/types/generated --client fetch",
    "generate:api-types:remote": "openapi-ts --input http://localhost:3000/api/v1/swagger_doc.json --output ./app/javascript/types/generated --client fetch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo-rails": "^8.0.16",
    "@popperjs/core": "^2.11.8",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "autoprefixer": "^10.4.21",
    "bootstrap": "^5.3.7",
    "bootstrap-icons": "^1.13.1",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "postcss-cli": "10.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.4",
    "sass": "^1.89.2",
    "tailwindcss": "^3.4.14"
  },
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "esbuild": "0.17.19",
    "typescript": "^5.3.3",
    "@hey-api/openapi-ts": "^0.45.0"
  }
}